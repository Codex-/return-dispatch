{
  "name": "return-dispatch",
  "version": "1.5.0",
  "private": true,
  "description": "Dispatch an action and output the run ID.",
  "main": "lib/main.js",
  "scripts": {
    "build:bundle": "npm run build:types && node ./esbuild.config.mjs",
    "build:types": "tsc",
    "format:check": "prettier --check **/*.ts",
    "format:write": "npm run format:check -- --write",
    "lint": "eslint --ext \".js,.ts\" .",
    "lint:fix": "npm run lint -- --fix",
    "release": "release-it",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codex-/return-dispatch.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Alex Miller",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.38",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.30.0",
    "chalk": "^5.0.1",
    "esbuild": "^0.14.42",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.6",
    "jest": "^28.1.1",
    "jest-circus": "^28.1.0",
    "prettier": "2.7.1",
    "release-it": "^15.1.1",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
