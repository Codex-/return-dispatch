  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/parser": "^4.31.2",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "jest": "^27.2.1",
    "jest-circus": "^27.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
